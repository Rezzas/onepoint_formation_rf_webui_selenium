{
	"name": "Test Docker Compose",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "robotframework",
	"workspaceFolder": "/opt/onepoint_formation_rf_webui_selenium",
	"mounts": [
		{
			"source": "${localWorkspaceFolder}",
			"target": "/opt/onepoint_formation_rf_webui_selenium",
			"type": "bind"
		}
	],
	"shutdownAction": "stopCompose",
	"customizations": {
		"vscode": {
			"extensions": [
				"d-biehl.robotcode"
			]
		},
		"settings": {
			"python.defaultInterpreterPath": "/usr/local/bin/python",
			"robotcode.robot.outputDir": "Resultats"
		}
	}
}